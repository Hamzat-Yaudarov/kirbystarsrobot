    "@prisma/client": "^5.6.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "axios": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "moment": "^2.29.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.15",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
